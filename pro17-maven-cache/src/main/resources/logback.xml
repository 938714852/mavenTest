<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss:SSS} [%thread] %-5level %level{100} - %msg%n</pattern>
        </encoder>
    </appender>
    <!--单独为一个包设置日志级别-->
    <logger name="com.ustc.maven.mapper" level="debug"></logger>
    <root level="debug">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>