<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ustc.maven.mapper.EmpMapper">
    <select id="queryEmp" resultType="com.ustc.maven.pojo.Emp" resultMap="empMap">
        select * from emp
    </select>
    <resultMap id="empMap" type="com.ustc.maven.pojo.Emp">
        <result property="did" column="dept_id"/>
        <result property="cjsj" column="create_time"/>
    </resultMap>

</mapper>